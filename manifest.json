{
  "name": "Tabhole",
  "version": "2.0.0",
  "manifest_version": 3,
  "description": "Put tabs in Tabhole and take them out where and when you need them.",
  "icons": {
    "16": "icons/empty-16.png",
    "48": "icons/empty-48.png",
    "128": "icons/empty-128.png"
  },
  "background": {
    "service_worker": "src/background.js",
    "type": "module"
  },
  "action": {
    "default_icon": "icons/empty-128.png",
    "default_title": "Tabhole - click to store tab"
  },
  "options_ui": {
    "page": "src/options/options.html",
    "open_in_tab": true
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}